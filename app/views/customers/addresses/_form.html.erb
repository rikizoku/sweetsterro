　<%= form_with model:address, url:'/addresses', local:true do |f| %>
　　　<div class= "d-flex form-group">
　　　  <label class= "col-2.5 p-2">郵便番号(ハイフンなし)</label>
　　　  <%= f.text_field :postal_code, placeholder: "0000000", class:"form-control w-25 " %>
　　　</div>
　　　<div class= "d-flex form-group">
　　　  <label class= "col-2 p-2">住所</label>
　　　  <%= f.text_field :address, placeholder: "東京都渋谷区代々木神薗町0-0", class:"form-control w- " %>
　　　</div>
　　　<div class= "d-flex form-group">
　　　  <label class= "col-2 p-2">宛名</label>
　　　  <%= f.text_field :name, placeholder: "令和道子", class:"form-control w-25 " %>
　　　</div>
　　　<div class="form-group-row text-end">
　　　  <%= f.submit '新規登録', class: "btn btn-success" %>
　　　</div>
　　　<% end %>